{
   "name": "fraas-edge-server",
   "version": "1.0.0",
   "description": "",
   "main": "src/server/index.js",
   "scripts": {
      "start": "concurrently --kill-others-on-fail \" npm run start:client\" \"npm run start:server\"",
      "start:client": "webpack --mode development --watch",
      "start:server": "nodemon --ignore './build/, ./src/universal/' --exec babel-node src/server/index.js --inspect",
      "clean": "rm -rf build && mkdir build",
      "test": "npm run lint && jest --coverage",
      "test:watch": "jest --watch",
      "lint": "./node_modules/eslint/bin/eslint.js src",
      "build": "npm run build:server && npm run build:client",
      "build:client": "webpack --mode production",
      "build:server": "npm run clean && babel -d ./build src/server -s"
   },
   "author": {
      "name": "David Polites",
      "email": "blckblttkd@gmail.com"
   },
   "license": "ISC",
   "browserslist": [
      "Chrome >= 70",
      "Firefox >= 60",
      "Explorer >= 11",
      "iOS >= 11",
      "Safari >= 11"
   ],
   "engines": {
      "node": ">=13.13.0",
      "npm": ">=6.0.0"
   },
